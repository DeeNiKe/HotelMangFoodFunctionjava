<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


	<servlet>
	     <servlet-name>food</servlet-name>
	     <servlet-class>com.food.FoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	      <servlet-name>food</servlet-name>
	      <url-pattern>/enter</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>insert</servlet-name> 
		<servlet-class>com.food.foodInsert</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insert</servlet-name>
      	<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>update</servlet-name>
		<servlet-class>com.food.UpdateFoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>update</servlet-name>
		<url-pattern>/up</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>com.food.DeleteFoodServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
</web-app>